<div class="page">
    <div class="page_content">
        <div class="row ">
            {{!-- <div class="card">
                <h1 class="text-center">{{page.title}}</h1>
            </div> --}}
            <div class="page_block">
            <header class="header row">
                <div class="col-3">
                   {{> logo}}
                </div>
                <div class="header_nav col-9">
                    <div class="header_nav_right">
                        {{> nav list=header}}
                    </div>
                </div>
            </header>
            {{> divider}}
            </div>
            <div class="page_block">
                <div class="hero">
                    {{> hero data=hero}}
                </div>
                
            </div>
            <div class="page_block">
                <main class="row">
                   
                        {{>  card}}
                    
                </main>
            </div>
            <div class="page_block">
                <div>
                    
                    <table class="table">
                        <tbody>
                            {{#each footer}}
                            <tr>
                                {{#each links}}
                                <td>{{text}}</td>
                                {{/each}}
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                    <caption>My table grow</caption>
                </div>
            </div>

            <div class="page_block">
            {{> divider}}
            <footer class="footer row">
                <div class="col-3">
                    {{> logo}}
                    <em class="footer_copyright">Copyright 2023</em>
                </div>
                {{#each footer}}
                <div class="col-3">
                    <span class="footer_title">{{title}}</span>
                    {{>nav list=links isVertical=true}}
                </div>
                    
                {{/each}}
            </footer>
            </div>
        </div>
    </div>
</div>